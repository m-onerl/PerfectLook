<script>
  fetch('/session-status', {
      method: 'GET',
      credentials: 'include'
    }).then(response => response.json())
      .then(data => {
        if (data.loggedIn) {
          // Użytkownik jest zalogowany, pokaż link wylogowania
          document.getElementById('logout-link').style.display = 'block';
        } else {
          // Użytkownik nie jest zalogowany, ukryj link wylogowania
          document.getElementById('logout-link').style.display = 'none';
        }
      });
</script>
